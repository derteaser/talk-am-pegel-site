{
  "name": "kuesters/talk-am-pegel",
  "description": "Talk am Pegel Website",
  "type": "project",
  "keywords": [
    "kirby",
    "cms",
    "event"
  ],
  "homepage": "https://www.talk-am-pegel.de",
  "require": {
    "php": "~8.2.0 || ~8.3.0",
    "getkirby/cms": "3.10.1.1",
    "getkirby/cli": "1.6.0",
    "leitsch/kirby-blade": "3.0.4",
    "wearejust/kirby-meta-tags": "3.0.1",
    "sylvainjule/locator": "1.1.4",
    "bnomei/kirby3-dotenv": "2.2.0",
    "bnomei/kirby3-robots-txt": "1.8.0",
    "thathoff/kirby-git-content": "5.2.0",
    "sentry/sdk": "4.0.0",
    "paulmorel/fathom-analytics": "1.0.0",
    "bnomei/kirby3-feed": "1.7.0",
    "lukaskleinschmidt/kirby-laravel-vite": "2.1.3"
  },
  "require-dev": {
    "lukaskleinschmidt/kirby-types": "1.1.2"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "allow-plugins": {
      "getkirby/composer-installer": true,
      "php-http/discovery": true
    }
  },
  "scripts": {
    "start": [
      "Composer\\Config::disableProcessTimeout",
      "@php -S localhost:8000 kirby/router.php"
    ],
    "post-autoload-dump": [
      "@php post-autoload-dump.php"
    ]
  }
}
